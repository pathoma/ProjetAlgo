generateur: matrice.o direction.o mot_mat.o outil.o mot.o premier_saisie.o generateur.o 
	gcc matrice.o direction.o mot_mat.o outil.o mot.o premier_saisie.o generateur.o -o generateur

matrice.o:matrice.c
	gcc -c matrice.c
	
direction.o:direction.c
	gcc -c direction.c
	
mot_mat.o:mot_mat.c
	gcc -c mot_mat.c
	
outil.o: outil.c
	gcc -c outil.c
	
mot.o: mot.c
	gcc -c mot.c
premier_saisie.o: premier_saisie.c
	gcc -c premier_saisie.c
generateur.o: generateur.c
	gcc -c generateur.c
